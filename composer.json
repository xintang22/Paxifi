{
  "name": "paxifi/api",
  "description": "Backend for Paxifi APP. Proudly powered by Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "laravel/framework": "4.2.*",
    "league/fractal": "0.7.0",
    "itsgoingd/clockwork": "v1.5",
    "aws/aws-sdk-php": "2.*",
    "paypal/rest-api-sdk-php": "v0.8.3",
    "lucadegasperi/oauth2-server-laravel": "1.0.x",
    "intervention/image": "*",
    "thujohn/pdf": "dev-master",
    "mike182uk/paypal-ipn-listener": "v2.0.0",
    "pda/pheanstalk": "*",
    "guzzlehttp/guzzle": "5.3.0",
    "cboden/ratchet": "0.3.*",
    "react/zmq": "0.2.*|0.3.*",
    "doctrine/dbal": "~2.5@dev",
    "graham-campbell/flysystem": "~0.7",
    "barryvdh/laravel-cors": "0.2.x",
    "stripe/stripe-php": "2.*",
    "sly/notification-pusher": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*",
    "mockery/mockery": "dev-master",
    "fzaninotto/faker": "1.4.*@dev",
    "behat/behat": "3.0.*@dev",
    "sami/sami": "*",
    "phploc/phploc": "*",
    "squizlabs/php_codesniffer": "1.*",
    "pdepend/pdepend": "1.1.0",
    "phpmd/phpmd": "1.4.*",
    "sebastian/phpcpd": "*"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests/TestCase.php"
    ],
    "psr-4": {
      "Paxifi\\": "app/src/Paxifi"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "repositories": [
    {"type": "composer", "url": "http://pkg.phpcomposer.com/repo/packagist/"},
    {"packagist": false}
  ]
}
